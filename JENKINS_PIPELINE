pipeline {
  agent any
    
  tools {nodejs "NodeJs"}
    
  stages {
            
    stage('Build') {
      steps {
        bat 'npm install'

      }
    }  

    stage('Deliver') {
      steps {
        bat './start.cmd'
        input message: 'Finished using the Battleship Server? (Click "Proceed" to continue)'
      }
  }
           
  }
}